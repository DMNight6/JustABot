  "scripts": {
    "build": "npm run prebuild:dev && tsc",
    "prebuild:dev": "rimraf dist",
    "start": "npm run pm2:lavalink && npm run pm2:bot",
    "pm2:bot": "pm2 start ./dist/src/index.js --name\"Bot\"",
    "pm2:lavalink": "pm2 start ./dist/src/scripts/lavalink.js --name \"Lavalink\"",
    "stop": "pm2 kill",
    "checkUpdate": "npx npm-check-updates -u && npm install --save"
  },
